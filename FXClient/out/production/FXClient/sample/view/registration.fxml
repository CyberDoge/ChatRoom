      </Label>
      <Label layoutX="77.0" layoutY="110.0" text="password">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="77.0" layoutY="145.0" text="confirm">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="loginField" layoutX="169.0" layoutY="74.0" />
      <PasswordField fx:id="passwordField" layoutX="169.0" layoutY="107.0" />
      <PasswordField fx:id="confirmField" layoutX="169.0" layoutY="142.0" />
      <Button fx:id="submitBtn" layoutX="278.0" layoutY="239.0" mnemonicParsing="false" onAction="#registerAction" text="Submit" />
      <Button layoutX="169.0" layoutY="239.0" mnemonicParsing="false" text="Cancel" onAction="#cancelAction"/>
